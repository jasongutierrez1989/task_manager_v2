<h1>Create a new User</h1>

<%= form_for :user, url: users_path do |f| %>
<br><%= f.label :name %>
<%= f.text_field :name %></br>
<br><%= f.label :job %>
<%= f.text_field :job %></br>
<br><%= f.label :email %>
<%= f.text_field :email %></br>
<br><%= f.label :password %>
<%= f.password_field :password %></br>
<br><%= f.submit %></br>
<% end %>
